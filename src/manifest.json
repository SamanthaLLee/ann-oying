{
  "name": "Anne Oying",
	"version": "1.0",
  "description" : "Hack the Northeast 2021 submission",
	"manifest_version": 2,
  "permissions": [
		"fontSettings",
    "tabs",
    "notifications",
		"http://*/*",
		"https://*/*",
    "topSites"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  }, 
	"content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
